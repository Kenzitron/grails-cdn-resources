<plugin name='cloud-resources' version='0.1' grailsVersion='1.3.5 &gt; *'>
  <author>Tomas Lin</author>
  <authorEmail>tomaslin@gmail.com</authorEmail>
  <title>Cloud Resources Plugin</title>
  <description>\
Loads static resources using Content Delivery Networks and off-site cloud storage using the Resources plugin framework
</description>
  <documentation>http://grails.org/plugin/cloud-resources</documentation>
  <resources>
    <resource>BuildConfig</resource>
    <resource>Config</resource>
    <resource>DataSource</resource>
    <resource>UrlMappings</resource>
    <resource>grails.plugin.cloudresources.CloudResourceMapper</resource>
    <resource>grails.plugin.cloudresources.CloudFoundryService</resource>
    <resource>grails.plugin.cloudresources.S3Service</resource>
  </resources>
  <dependencies>
    <plugin name='resources' version='1.0-alpha11 &gt; *' />
  </dependencies>
  <behavior />
</plugin>